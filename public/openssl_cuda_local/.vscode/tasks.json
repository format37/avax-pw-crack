{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "profile public_key_derivation",
            "type": "shell",
            "command": "ncu",
            "args": [
                "--target-processes", "all",
                "--profile-from-start", "off",
                "--force-overwrite",
                "-o", "profile",
                "${workspaceFolder}/program"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [],
            "dependsOn": [
                "nvcc build active file"
            ]
        },
        {
            "label": "nvcc public_key_derivation",
            "type": "shell",
            "command": "/bin/bash",
            "args": [
                "-c",
                "source /mnt/hdd0/alex/anaconda3/bin/activate && conda activate crack && nvcc -diag-suppress 1444 -G -g ${workspaceFolder}/public_openssl_cuda_local.cu -arch=sm_86 -I/home/alex/projects/avax-pw-crack/public/openssl_cuda_local/include/ -o program"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$nvcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "dedicated",
                "showReuseMessage": true,
                "clear": false
            }
        },
        {
            "label": "nvcc bn_div",
            "type": "shell",
            "command": "/bin/bash",
            "args": [
                "-c",
                "source /mnt/hdd0/alex/anaconda3/bin/activate && conda activate crack && nvcc -diag-suppress 1444 -G -g ${workspaceFolder}/tests/bn_divide/test.cu -arch=sm_86 -I/home/alex/projects/avax-pw-crack/public/openssl_cuda_local/include/ -o program"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$nvcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "dedicated",
                "showReuseMessage": true,
                "clear": false
            }
        }
    ]
}